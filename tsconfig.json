{
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./lib",
    "target": "es5",
    "lib": [
      "dom", "es5", "es6"
    ],
    "module": "commonjs",
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "experimentalDecorators": true,
    "jsx": "react"
  },
  "exclude": [
    "node_modules"
  ]
}